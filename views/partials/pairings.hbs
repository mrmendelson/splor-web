<div class="col-xs-9 pairings">
  <!-- PAIRINGS -->
  <div class="dash_pairings">
    <div class="dash_header">
      <div class="dash_header-left">
        <h2 class="float-left">Pairings</h2>
        <ul class="dash_header float-left">
          <li><a href="javascript:void(0);" class="refresh-pairings" data-id={{class.id}}><span class="glyphicon glyphicon-refresh spinner"></span>Generate</a></li>
          <li class="last"><a href="javascript:void(0);"><span class="glyphicon glyphicon-plus"></span>Add Custom</a></li>
        </ul>
      </div>
      <a href="javascript:void(0);" class="splor-button" data-id="{{class.id}}"><img src="/img/btn_splor-all.png"></a>
    </div><!--/dash_header-->
    {{#each pairings}}
    <div class="dash_item-wrapper generated clear-both">
      <div class="dash_item skill col-md-2 col-xs-12">
        <h4>Skill</h4>
        <p>{{Exercise.name}}</p>
      </div><!--/dash_item skill-->
      <div class="dash_item students col-md-7 col-xs-12">
        <h4>Students</h4>
        <div class="dash_item student">
          <img class="dash_item tutor" src="{{Tutor.avatar}}">
          <h3 class="student-name">{{Tutor.name}}</h3>
          <span>tutor</span>
        </div><!--/dash_item student-->
        <div class="dash_item student">
          <img class="dash_item tutee" src="{{Tutee.avatar}}">
          <h3 class="student-name">{{Tutee.name}}</h3>
          <span>tutee</span>
        </div><!--/dash_item student-->
      </div><!--/dash_item students-->
      <form action="/pairings/{{id}}/delete" method="post">
        <button type="submit" class="btn btn-danger">Delete</button>
      </form>
    </div><!--/dash-item-wrapper-->
    {{else}}
    <div class="dash_item-wrapper generated clear-both">
      <h3>No pairings found.</h3>
      <a href="javascript:void(0);" class="refresh-pairings" data-id={{class.id}}><span class="glyphicon glyphicon-refresh spinner"></span>Generate now</a>
    </div>
    {{/each}}
  </div><!--/dash_pairings-->
  <!-- /PAIRINGS -->
</div><!--/main-->
