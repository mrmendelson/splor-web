if [ -z "$1" ]
  then
  echo "refresh-students requires a first argument of an authenticated user ID."
  exit 1
fi
DEBUG=splor*,$2 node -e "require('./lib/shared/refresh-students.js').withId("$1", function(err) { if (err) { console.error('FAILED: ', err.message); process.exit(1) } else { process.exit(0) } })"
